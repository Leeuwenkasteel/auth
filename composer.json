{
    "name": "leeuwenkasteel/auth",
    "description": "authentication for leeuwenkasteel packages",
    "type": "package",
    "license": "mit",
    "authors": [
        {
            "name": "Sanne van Leeuwen",
            "email": "sanne@leeuwenkasteel.nl"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "leeuwenkasteel/form": "1.0.x-dev",
        "leeuwenkasteel/table": "1.0.x-dev",
        "leeuwenkasteel/helpers": "1.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Leeuwenkasteel\\Auth\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leeuwenkasteel\\Auth\\AuthServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
